{
  "expo": {
    "name": "VStore",
    "slug": "VStore",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#555CC4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "#555CC4",
      "translucent": false,
      "barStyle": "light-content"
    },
    "android": {
      "package": "com.vistanasoft.astore",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDREwNclzAjMIIypa6GEUOUZOTeozBh2VI"
        }
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": []
    },
    "notification": {
      "icon": "./assets/images/icon.png"
    }
  }
}
